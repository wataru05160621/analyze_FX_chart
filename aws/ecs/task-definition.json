{
  "family": "analyze-fx",
  "taskRoleArn": "arn:aws:iam::455931011903:role/analyze-fx-task-role",
  "executionRoleArn": "arn:aws:iam::455931011903:role/analyze-fx-execution-role",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "analyze-fx",
      "image": "455931011903.dkr.ecr.ap-northeast-1.amazonaws.com/analyze-fx:latest",
      "essential": true,
      "environment": [
        {"name": "APP_NAME", "value": "analyze-fx"},
        {"name": "TZ", "value": "Asia/Tokyo"},
        {"name": "PAIR", "value": "USDJPY"},
        {"name": "SYMBOL", "value": "USD/JPY"},
        {"name": "TIMEFRAMES", "value": "5m,1h"},
        {"name": "S3_PREFIX", "value": "charts"},
        {"name": "LOG_LEVEL", "value": "INFO"},
        {"name": "DATA_SOURCE", "value": "twelvedata"},
        {"name": "AWS_DEFAULT_REGION", "value": "ap-northeast-1"}
      ],
      "secrets": [
        {
          "name": "NOTION_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:455931011903:secret:NOTION_API_KEY"
        },
        {
          "name": "NOTION_DB_ID",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:455931011903:secret:NOTION_DB_ID"
        },
        {
          "name": "SLACK_WEBHOOK_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:455931011903:secret:SLACK_WEBHOOK_URL"
        },
        {
          "name": "S3_BUCKET",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:455931011903:secret:S3_BUCKET"
        },
        {
          "name": "TWELVEDATA_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:455931011903:secret:TWELVEDATA_API_KEY"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/analyze-fx",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}