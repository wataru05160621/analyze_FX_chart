{
  "family": "analyze-fx",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::455931011903:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::455931011903:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "analyze-fx",
      "image": "455931011903.dkr.ecr.ap-northeast-1.amazonaws.com/analyze-fx:latest",
      "essential": true,
      "entryPoint": ["python", "-m", "src.runner.main_v2"],
      "environment": [
        {"name": "APP_NAME", "value": "analyze-fx"},
        {"name": "TZ", "value": "Asia/Tokyo"},
        {"name": "PAIR", "value": "USDJPY"},
        {"name": "SYMBOL", "value": "USD/JPY"},
        {"name": "TIMEFRAMES", "value": "5m,1h"},
        {"name": "S3_PREFIX", "value": "charts"},
        {"name": "LOG_LEVEL", "value": "INFO"},
        {"name": "DATA_SOURCE", "value": "twelvedata"},
        {"name": "ENABLE_TWITTER", "value": "false"},
        {"name": "ENABLE_WORDPRESS", "value": "false"},
        {"name": "TWITTER_MIN_EV_R", "value": "0.5"},
        {"name": "TWITTER_MIN_CONFLUENCE", "value": "4"},
        {"name": "TWITTER_MIN_CONFIDENCE", "value": "high"},
        {"name": "WORDPRESS_AUTO_PUBLISH", "value": "false"},
        {"name": "WORDPRESS_CATEGORY", "value": "FX Analysis"}
      ],
      "secrets": [
        {
          "name": "NOTION_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:455931011903:secret:analyze-fx/notion-api-key:NOTION_API_KEY::"
        },
        {
          "name": "NOTION_DB_ID",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:455931011903:secret:analyze-fx/notion-db-id:NOTION_DB_ID::"
        },
        {
          "name": "SLACK_WEBHOOK_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:455931011903:secret:analyze-fx/slack-webhook:SLACK_WEBHOOK_URL::"
        },
        {
          "name": "S3_BUCKET",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:455931011903:secret:analyze-fx/s3-bucket:S3_BUCKET::"
        },
        {
          "name": "TWELVEDATA_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:455931011903:secret:analyze-fx/twelvedata-api-key:TWELVEDATA_API_KEY::"
        },
        {
          "name": "TWITTER_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:455931011903:secret:analyze-fx/twitter:TWITTER_API_KEY::"
        },
        {
          "name": "TWITTER_API_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:455931011903:secret:analyze-fx/twitter:TWITTER_API_SECRET::"
        },
        {
          "name": "TWITTER_ACCESS_TOKEN",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:455931011903:secret:analyze-fx/twitter:TWITTER_ACCESS_TOKEN::"
        },
        {
          "name": "TWITTER_ACCESS_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:455931011903:secret:analyze-fx/twitter:TWITTER_ACCESS_SECRET::"
        },
        {
          "name": "TWITTER_BEARER_TOKEN",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:455931011903:secret:analyze-fx/twitter:TWITTER_BEARER_TOKEN::"
        },
        {
          "name": "WORDPRESS_API_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:455931011903:secret:analyze-fx/wordpress:WORDPRESS_API_URL::"
        },
        {
          "name": "WORDPRESS_USERNAME",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:455931011903:secret:analyze-fx/wordpress:WORDPRESS_USERNAME::"
        },
        {
          "name": "WORDPRESS_APP_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:455931011903:secret:analyze-fx/wordpress:WORDPRESS_APP_PASSWORD::"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/analyze-fx",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}